<mapper namespace="com.summit.dao.repository.AwakenlockRecordsDao">
  <resultMap id="BaseResultMap" type="com.summit.dao.entity.AwakenlockRecords">
    <result column="awak_id" jdbcType="VARCHAR" property="awakId" />
